<div class="container px-5 px-sm-0">
  <div class="row">
    
    
    <%= render "books/list", user: @user, book_new: @book_new %>



<div class="col-md-8 offset-md-1">
      <h2>Users</h2>
      <table class="table">
        <thead>
          <tr>
            <th>image</th>
            <th>name</th>
            <th>colspan="3"</th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |users| %>
          <tr>
            <td><%= attachment_image_tag users, :profile_image, :fill, 60, 60, fallback: "no_image.jpg", size: "100x100" %></td>
            <td><%= users.name %></td>
            <td><%= link_to "Show", user_path(users) %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
